#include <stdio.h>

int main() {

  int a, b, c;
  printf("Insira os Valores de A, B e C Respectivamente: ");
  scanf("%d %d %d", &a, &b, &c);

  if((a + b) > c && (c + b) > a && (a + c) > b){
    printf("Os valores informados sao de um Triangulo");
     if(a == b && b == c){
    printf("\nUm Triangulo e Equilatero");
    }else if (a == b || c == b || a == c){
      printf("\nUm Triangulo e Isoceles");
    }else{
       printf("\nUm Triangulo e Escaleno");
    }
  }
  else{
      printf("Nao e Triangulo");
   }
}
