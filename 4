#include <stdio.h>
#include <math.h>
int main() {

  int a, b, c;
  printf("Insira os Valores de A, B e C Respectivamente: ");
  scanf("%d %d %d", &a, &b, &c);

  if((a + b) > c && (c + b) > a && (a + c) > b){
    printf("Os valores informados sao de um Triangulo");
    if(sqrt(a) == sqrt(b) + sqrt(c)|| sqrt(b) == sqrt(a) + sqrt(c) || sqrt(c) == sqrt(a) + sqrt(b)){
      printf(" Retangulo");
    }else if (sqrt(a) > sqrt(b) + sqrt(c) || sqrt(b) > sqrt(a) + sqrt(c) || sqrt(c) > sqrt(a) + sqrt(b)){
      printf(" Obtusangulo");
    }else{
      printf(" Acutangulo");
    }
  }
  else{
      printf("Nao e Triangulo");
   }
}
