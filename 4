#include <stdio.h>
int main() {

  int a, b, c;
  printf("Insira os Valores de A, B e C Respectivamente: ");
  scanf("%d %d %d", &a, &b, &c);

  if((a + b) > c && (c + b) > a && (a + c) > b){
    printf("Os valores informados sao de um Triangulo");
    if(a * a == b * b + c * c|| b * b == a * a + c * c || c * c == a * a + b * b){
      printf(" Retangulo");
    }else if (a * a > b * b + c* c || b * b > a * a + c * c || c * c > a * a + b * b){
      printf(" Obtusangulo");
    }else{
      printf(" Acutangulo");
    }
  }
  else{
      printf("Nao e Triangulo");
   }
}
